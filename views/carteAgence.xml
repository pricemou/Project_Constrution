<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="carteAgence">
            <html lang="en">

            <head>
                <meta charset="UTF-8" />
                <meta http-equiv="X-UA-Compatible" content="IE=edge" />
                <meta name="viewport" content="width=device-width, initial-scale=1.0" />
                <title>Carte agence</title>
                <!-- Elegant font icons -->
                <link href="/web_cash_out/static/assets/vendor/materializeicon/material-icons.css" rel="stylesheet" />

                <!-- Swiper Slider -->
                <link href="/web_cash_out/static/assets/vendor/swiper/css/swiper.min.css" rel="stylesheet" />
                <link rel="stylesheet"
                    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
                <!-- Custom styles for this template -->
                <link href="/web_cash_out/static/assets/css/style-blue.css" rel="stylesheet" id="style" />
                <link href="/web_cash_out/static/assets/css/agence_maps.css " rel="stylesheet " id="style" />
                <link href="/web_cash_out/static/assets/css/agence.css " rel="stylesheet " id="style" />
                <link href="/web_cash_out/static/assets/css/styleIndicator.css" rel="stylesheet" />
                <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
                <script src="/web_cash_out/static/assets/js/cashout.js"></script>

            </head>
            <header class="header fixed-top header-fill">
                <nav class="navbar">
                    <div>
                        <button class="menu-btn btn btn-link btn-44" onclick="window.history.go(-1); return false;">
                            <span class="icon material-icons">arrow_back</span>
                        </button>
                    </div>
                    <div>
                        <h6 class="text"><span>Trouver une agence</span></h6>
                    </div>
                    <div>
                    </div>
                </nav>
            </header>

            <body>
                <div class=" diffg">
                    <section class="step-indicator ">
                        <div class="step step1 active">
                            <div class="step-icon">1</div>
                        </div>
                        <div class="indicator-line active">

                        </div>
                        <div class="step step2 active">
                            <div class="step-icon">2</div>
                        </div>
                        <div class="indicator-line active"></div>
                        <div class="step step3 active">
                            <div class="step-icon">3</div>
                        </div>
                        <div class="indicator-line active">

                        </div>
                        <div class="step step3">
                            <div class="step-icon">3</div>
                        </div>
                    </section>
                </div>



                <div id="map">
                </div>
                <!-- modal code -->
                <form action="/cashout/itineraire/" method="post">
                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />
                    <input type="hidden" name="agence_id" class="agence_id"/>
                    <input type="hidden" name="agence_name" class="agence_name"/>
                    <input type="hidden" name="agence_address" class="agence_address"/>
                    <input type="hidden" name="agence_longitude" class="agence_longitude"/>
                    <input type="hidden" name="agence_latitude" class="agence_latitude"/>
                    <div class="modal fade modals" id="exampleModal" tabindex="-1" role="dialog"
                        aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog modal-cusom " role="document ">
                            <div class="modal-content modal-contens ">
                                <div class="modal-header modal-border head ">
                                    <h5 class="modal-title " id="exampleModalLabel "> <span class="material-icons "
                                            data-dismiss="modal ">
                                            delete_forever
                                        </span>
                                    </h5>
                                </div>
                                <div class="modal-body text-center ">
                                    <img src="" class="image" />
                                    <h2>Nom ,Prenom</h2>
                                    <span id="address"><i class="icon material-icons place " aria-hidden="true ">place</i> Adresse</span>
                                </div>
                                <div class="text-center ">
                                    <span><i class="icon material-icons flag " aria-hidden="true ">flag</i> Agence
                                        public</span>

                                </div>
                                <div class="text-center ">
                                    <span> 10-15min</span>
                                    <span><i class="icon material-icons star " aria-hidden="true ">star</i> </span>
                                    <span class="ps ">4.0</span>

                                </div>
                                <div class="modal-footer modal-border buttons ">
                                    <button type="button " class="btn btn-dark valider " id="valider">Confirmer</button>
                                </div>

                            </div>
                        </div>
                    </div>
                </form>



                <!-- Required jquery and libraries -->
                <script src="/web_cash_out/static/assets/js/jquery-3.3.1.min.js "></script>
                <script src="/web_cash_out/static/assets/js/popper.min.js "></script>
                <script src="/web_cash_out/static/assets/vendor/bootstrap-4.4.1/js/bootstrap.min.js "></script>

                <!-- cookie css -->

                <!-- Customized jquery file  -->
                <script src="/web_cash_out/static/assets/js/main.js "></script>
                <script src="/web_cash_out/static/assets/js/color-scheme-demo.js "></script>
                <script>
                    document.getElementById('valider').onclick = function () {
                        location.href = "felicitation"
                    }

                    function close() {

                    }
                </script>
                <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCc-7cU3-_x1VTV5eW3g2pVnl3vi9lvv7w&amp;callback=initMap&amp;libraries=&amp;v=weekly"></script>

            </body>

            </html>

